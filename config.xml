<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ionic.starter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EustoryApp</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/eustoryApp-ldpi.png" />
        <icon density="mdpi" src="resources/android/icon/eustoryApp-mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/eustoryApp-hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/eustoryApp-xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/eustoryApp-xxhdpi.png" />
        <icon density="xxxhdpi" src="resources/android/icon/eustoryApp-xxxhdpi.png" />
        <splash density="land-ldpi" src="resources/android/splash/eustoryApp-Land-ldpi.png" />
        <splash density="land-mdpi" src="resources/android/splash/eustoryApp-Land-mdpi.png" />
        <splash density="land-hdpi" src="resources/android/splash/eustoryApp-Land-hdpi.png" />
        <splash density="land-xhdpi" src="resources/android/splash/eustoryApp-Land-xhdpi.png" />
        <splash density="land-xxhdpi" src="resources/android/splash/eustoryApp-Land-xxhdpi.png" />
        <splash density="land-xxxhdpi" src="resources/android/splash/eustoryApp-Land-xxxhdpi.png" />
        <splash density="port-ldpi" src="resources/android/splash/eustoryApp-Port-ldpi.png" />
        <splash density="port-mdpi" src="resources/android/splash/eustoryApp-Port-mdpi.png" />
        <splash density="port-hdpi" src="resources/android/splash/eustoryApp-Port-hdpi.png" />
        <splash density="port-xhdpi" src="resources/android/splash/eustoryApp-Port-xhdpi.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/eustoryApp-Port-xxhdpi.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/eustoryApp-Port-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/icon/eusoryApp-Icon.png" width="57" />
        <icon height="114" src="resources/ios/icon/eusoryApp-Icon@2x.png" width="114" />
        <icon height="40" src="resources/ios/icon/eusoryApp-Icon-40.png" width="40" />
        <icon height="80" src="resources/ios/icon/eusoryApp-Icon-40@2x.png" width="80" />
        <icon height="120" src="resources/ios/icon/icon-40@3x.png" width="120" />
        <icon height="50" src="resources/ios/icon/eusoryApp-Icon-50.png" width="50" />
        <icon height="100" src="resources/ios/icon/eusoryApp-Icon-50@2x.png" width="100" />
        <icon height="60" src="resources/ios/icon/icon-60.png" width="60" />
        <icon height="120" src="resources/ios/icon/eusoryApp-Icon-60@2x.png" width="120" />
        <icon height="180" src="resources/ios/icon/eusoryApp-Icon-60@3x.png" width="180" />
        <icon height="72" src="resources/ios/icon/eusoryApp-Icon-72.png" width="72" />
        <icon height="144" src="resources/ios/icon/eusoryApp-Icon-72@2x.png" width="144" />
        <icon height="76" src="resources/ios/icon/eusoryApp-Icon-76.png" width="76" />
        <icon height="152" src="resources/ios/icon/eusoryApp-Icon-76@2x.png" width="152" />
        <icon height="167" src="resources/ios/icon/eusoryApp-Icon-83.5@2x.png" width="167" />
        <icon height="29" src="resources/ios/icon/eusoryApp-Icon-Small.png" width="29" />
        <icon height="58" src="resources/ios/icon/eusoryApp-Icon-Small@2x.png" width="58" />
        <icon height="87" src="resources/ios/icon/eusoryApp-Icon-Small@3x.png" width="87" />
        <icon height="1024" src="resources/ios/icon/icon-1024.png" width="1024" />
        <splash height="1136" src="resources/ios/splash/eustoryApp-568h@2x~.png" width="640" />
        <splash height="1334" src="resources/ios/splash/eustoryApp-667h.png" width="750" />
        <splash height="2208" src="resources/ios/splash/eustoryApp-736h.png" width="1242" />
        <splash height="1242" src="resources/ios/splash/eustoryApp-Landscape-736h.png" width="2208" />
        <splash height="1536" src="resources/ios/splash/eustoryApp-Landscape@2x~ipad.png" width="2048" />
        <splash height="2048" src="resources/ios/splash/eustoryApp-Landscape@~ipadPro.png" width="2732" />
        <splash height="768" src="resources/ios/splash/eustoryApp-Landscape~ipad.png" width="1024" />
        <splash height="2048" src="resources/ios/splash/eustoryApp-Portrait@2x~ipad.png" width="1536" />
        <splash height="2732" src="resources/ios/splash/eustoryApp-Portrait@~ipadPro.png" width="2048" />
        <splash height="1024" src="resources/ios/splash/eustoryApp-Portrait~ipad.png" width="768" />
        <splash height="960" src="resources/ios/splash/eustoryApp@2x~.png" width="640" />
        <splash height="480" src="resources/ios/splash/eustoryApp~iphone.png" width="320" />
        <splash height="2732" src="resources/ios/splash/eustoryApp@2x~universal~anyany.png" width="2732" />
    </platform>
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="1.1.19" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="2.0.5" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="com-sarriaroman-photoviewer" spec="^1.1.16" />
    <plugin name="phonegap-plugin-barcodescanner" spec="^7.1.2">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <engine name="android" spec="^7.0.0" />
</widget>
